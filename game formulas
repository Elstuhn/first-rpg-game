attack done range:
  min atk = self.atk
  max atk = (self.atk*2)/1.5
  
  
  
class Player():
	def __init__(self, hp : int, def : int, attack : int, dung : list):
		self.hp = hp
		self.def = def
		self.atk = attack
		self.curhp = hp
		self.dun = dung

